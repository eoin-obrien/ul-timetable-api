{
  "name": "ul-timetable-api",
  "version": "0.2.1",
  "description": "GraphQL API for the University of Limerick's timetable.",
  "repository": {
    "type": "git",
    "url": "https://github.com/videtur/ul-timetable-api.git"
  },
  "author": "Eoin O'Brien <eoinobrien910@gmail.com> (https://videtur.io/)",
  "license": "GPL-3.0+",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "nodemon --watch dist dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --watch dist --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "apollo-server-lambda": "^1.3.2",
    "body-parser": "^1.17.2",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.4",
    "dataloader": "^1.3.0",
    "dotenv": "^2.0.0",
    "express": "^4.15.4",
    "graphql": "^0.10.5",
    "graphql-date": "^1.0.3",
    "graphql-server-express": "^1.1.0",
    "graphql-tools": "^1.2.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.6.6",
    "morgan": "^1.7.0",
    "parse-numeric-range": "0.0.2",
    "request": "^2.78.0",
    "request-promise-native": "^1.0.4",
    "title-capitalization": "0.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/cheerio": "^0.22.2",
    "@types/connect-mongo": "0.0.32",
    "@types/cors": "^2.8.1",
    "@types/dotenv": "^2.0.20",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/graphql": "^0.10.2",
    "@types/graphql-date": "^1.0.1",
    "@types/lodash": "^4.14.63",
    "@types/mongodb": "^2.1.43",
    "@types/mongoose": "^4.7.9",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.12",
    "@types/request": "0.0.45",
    "@types/request-promise-native": "^1.0.6",
    "concurrently": "^3.4.0",
    "nodemon": "^1.11.0",
    "shelljs": "^0.7.7",
    "tslint": "^5.0.0",
    "tslint-config-airbnb": "^5.2.1",
    "typescript": "^2.4.0"
  }
}
