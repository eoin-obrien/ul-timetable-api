{
  "name": "ul-timetable-api",
  "version": "0.2.1",
  "description": "GraphQL API for the University of Limerick's timetable (www.timetable.ul.ie).",
  "keywords": [
    "graphql",
    "university",
    "limerick",
    "university of limerick",
    "ul",
    "timetable",
    "webscraper"
  ],
  "engines": {
    "node": ">= 8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eoin-obrien/ul-timetable-api.git"
  },
  "homepage": "https://github.com/eoin-obrien/ul-timetable-api",
  "bugs": {
    "url": "https://github.com/eoin-obrien/ul-timetable-api/issues"
  },
  "author": "Eoin O'Brien <eoinobrien910@gmail.com> (https://videtur.io/)",
  "license": "GPL-3.0",
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "pretest": "npm run lint",
    "test": "npm run test-only",
