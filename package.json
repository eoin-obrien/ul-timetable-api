{
  "name": "ul-timetable-api",
  "version": "0.2.1",
  "description": "GraphQL API for the University of Limerick's timetable.",
  "repository": {
    "type": "git",
    "url": "https://github.com/videtur/ul-timetable-api.git"
  },
  "author": "Eoin O'Brien <eoinobrien910@gmail.com> (https://videtur.io/)",
  "license": "GPL-3.0+",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "npm run build-ts && npm run tslint",
    "serve": "nodemon --watch dist dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --watch dist --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "apollo-server-lambda": "^1.3.2",
    "cheerio": "^1.0.0-rc.2",
    "dataloader": "^1.3.0",
    "graphql": "^0.12.3",
    "graphql-date": "^1.0.3",
    "graphql-tools": "^2.18.0",
    "parse-numeric-range": "^0.0.2",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "title-capitalization": "^0.0.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/graphql": "^0.12.1",
    "@types/graphql-date": "^1.0.2",
    "@types/node": "^9.3.0",
    "@types/request-promise-native": "^1.0.10",
    "concurrently": "^3.5.1",
    "nodemon": "^1.14.11",
    "shelljs": "^0.8.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.4.2",
    "typescript": "^2.6.2"
  }
}
