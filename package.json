    "@types/aws-lambda": "0.0.22",
    "@types/node": "^8.0.57",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "rimraf": "^2.6.2",
    "serverless-webpack": "^4.0.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^2.3.7",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tsutils": "^2.27.1",
    "typescript": "^2.6.2",
    "webpack": "^3.6.0"
  }
